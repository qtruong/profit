<project default="all">
  <property name="src" location="src"/>
  <property name="build" location="../class"/>

  <target name="init">
    <mkdir dir="${build}"/>
  </target>

  <target name="all" depends="init">
    <javac srcdir="${src}" destdir="${build}" debug="on" deprecation="on"/>
  </target>

  <target name="clean">
    <delete dir="${build}/com/qtt/app/stock/simulator"/>
  </target>

</project>
